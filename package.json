{
  "name": "gatsby-starter-blog",
  "private": true,
  "description": "A starter for a paginated blog powered by Gatsby and Markdown",
  "version": "1.1.0",
  "author": "Nicky Meuleman",
  "bugs": {
    "url": "https://github.com/NickyMeuleman/gatsby-paginated-blog/issues"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.14.0",
    "gatsby-image": "^2.2.14",
    "gatsby-plugin-feed": "^2.3.9",
    "gatsby-plugin-google-analytics": "^2.1.11",
    "gatsby-plugin-manifest": "^2.2.9",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-sharp": "^2.2.18",
    "gatsby-plugin-styled-components": "^3.1.3",
    "gatsby-plugin-typography": "^2.3.5",
    "gatsby-remark-copy-linked-files": "^2.1.13",
    "gatsby-remark-images": "^3.1.19",
    "gatsby-remark-prismjs": "^3.3.9",
    "gatsby-remark-responsive-iframe": "^2.2.10",
    "gatsby-remark-smartypants": "^2.1.5",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-remark": "^2.6.19",
    "gatsby-transformer-sharp": "^2.2.12",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.7.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.3.2",
    "styled-media-query": "^2.1.2",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2016": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^6.2.2",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.18.2"
  },
  "homepage": "https://github.com/NickyMeuleman/gatsby-paginated-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickyMeuleman/gatsby-paginated-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "deploy": "gh-pages -d public"
  }
}
